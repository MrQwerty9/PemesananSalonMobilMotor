<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.sstudio.otocare">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.sstudio.otocare.ui.login.LoginActivity">
        </activity>
        <activity android:name="com.sstudio.otocare.ui.booking.BookingActivity"></activity>
        <activity android:name="com.sstudio.otocare.CobaActivity">
        </activity>
        <activity android:name="com.sstudio.otocare.ui.home.HomeActivity"></activity>

        <activity android:name="com.sstudio.otocare.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>